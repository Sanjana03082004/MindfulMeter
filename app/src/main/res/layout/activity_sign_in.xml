<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignIn">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="118dp"
        android:layout_height="150dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="4dp"
        android:contentDescription="todo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textAlignment="center"
        android:text="Sign Up Here\nCreate new account"
        android:textColor="@color/blue"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="DuplicateIds" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="370dp"
        android:layout_height="57dp"
        android:autofillHints=""
        android:background="@drawable/edittext"
        android:drawableStart="@drawable/user"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:text="Name"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:ignore="LabelFor,RtlSymmetry"
        tools:layout_editor_absoluteX="39dp" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="370dp"
        android:layout_height="57dp"
        android:autofillHints=""
        android:background="@drawable/edittext"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:text="Age"
        app:layout_constraintTop_toBottomOf="@+id/editTextText"
        tools:ignore="LabelFor,RtlSymmetry"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/editTextText1"
        android:layout_width="370dp"
        android:layout_height="57dp"
        android:autofillHints=""
        android:background="@drawable/edittext"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:inputType="text"
        android:paddingStart="15dp"
        android:text="Gender"
        app:layout_constraintTop_toBottomOf="@+id/editTextText2"
        tools:ignore="LabelFor,RtlSymmetry"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="370dp"
        android:layout_height="57dp"
        android:autofillHints=""
        android:background="@drawable/edittext"
        android:drawableStart="@drawable/mail"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:paddingStart="15dp"
        android:text="Email"
        app:layout_constraintEnd_toEndOf="@+id/editTextText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editTextText"
        app:layout_constraintTop_toBottomOf="@+id/editTextText1"
        tools:ignore="LabelFor,RtlSymmetry" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="370dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:autofillHints=""
        android:background="@drawable/edittext"
        android:drawableStart="@drawable/lock"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:inputType="textPassword"
        android:paddingStart="15dp"
        android:text="password1"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextEmailAddress2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/editTextTextEmailAddress2"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress2"
        tools:ignore="LabelFor,MissingConstraints,RtlSymmetry" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:backgroundTint="@color/blue"
        android:text="create_account"
        app:layout_constraintEnd_toEndOf="@+id/land"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="@+id/land"
        app:layout_constraintTop_toBottomOf="@+id/land"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="already have an account?"
        android:textColor="@color/blue"
        app:layout_constraintEnd_toEndOf="@+id/button4"
        app:layout_constraintStart_toStartOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

    <EditText
        android:id="@+id/land"
        android:layout_width="370dp"
        android:layout_height="57dp"
        android:layout_marginTop="12dp"
        android:autofillHints=""
        android:background="@drawable/edittext"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:paddingStart="15dp"
        android:text="Phone number"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword"
        tools:ignore="RtlSymmetry" />

</androidx.constraintlayout.widget.ConstraintLayout>